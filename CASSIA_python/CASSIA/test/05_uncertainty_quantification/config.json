{
  "test_name": "test_uncertainty_quantification",
  "description": "Test annotation stability through multiple stochastic runs",
  "model": "google/gemini-2.5-flash-preview",
  "provider": "openrouter",
  "temperature": 0.7,
  "data_file": "processed.csv",
  "method_params": {
    "n_iterations": 5,
    "tissue": "large intestine",
    "species": "human",
    "max_workers": 4,
    "n_genes": 50,
    "n_clusters_subset": 3,
    "output_name": "uq_test"
  },
  "validation": {
    "check_all_runs_complete": true,
    "check_similarity_scores": true,
    "min_similarity": 0.5,
    "max_similarity": 1.0,
    "check_stability_metrics": true
  },
  "reporting": {
    "save_logs": true,
    "verbose": true,
    "save_individual_runs": true,
    "save_similarity_matrix": true,
    "save_stability_report": true,
    "show_annotation_variance": true
  }
}
